<h1>EDIT THINGS</h1>
<form id="edit-item-form" method="post" action="/users/{{result.user_id}}/{{result.id}}/edit/">
  <input name="todo" type="text" value="{{result.todo}}"></input>
  <input name="description" type="text" value="{{result.description}}"></input>
  <input name="length" style="display:none" value="{{result.length}}"></input>
  <input type="submit" value="Submit"></input>
</form>
